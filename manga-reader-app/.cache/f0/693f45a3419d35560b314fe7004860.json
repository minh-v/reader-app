ource":"pages/Home/Home.jsx","original":{"line":70,"column":7}},{"generated":{"line":109,"column":19},"source":"pages/Home/Home.jsx","original":{"line":70,"column":20}},{"generated":{"line":109,"column":23},"source":"pages/Home/Home.jsx","original":{"line":70,"column":24}},{"generated":{"line":109,"column":52},"source":"pages/Home/Home.jsx","original":{"line":70,"column":25}},{"generated":{"line":109,"column":73},"source":"pages/Home/Home.jsx","original":{"line":70,"column":24}},{"generated":{"line":110,"column":0},"source":"pages/Home/Home.jsx","original":{"line":70,"column":38}},{"generated":{"line":110,"column":4},"source":"pages/Home/Home.jsx","original":{"line":70,"column":38}},{"generated":{"line":110,"column":9},"source":"pages/Home/Home.jsx","original":{"line":70,"column":43}},{"name":"selectedManga","generated":{"line":110,"column":11},"source":"pages/Home/Home.jsx","original":{"line":70,"column":45}},{"generated":{"line":111,"column":0},"source":"pages/Home/Home.jsx","original":{"line":70,"column":24}},{"generated":{"line":111,"column":4},"source":"pages/Home/Home.jsx","original":{"line":64,"column":4}},{"generated":{"line":111,"column":5},"source":"pages/Home/Home.jsx","original":{"line":63,"column":2}},{"generated":{"line":112,"column":0},"source":"pages/Home/Home.jsx","original":{"line":73,"column":1}},{"generated":{"line":112,"column":1},"source":"pages/Home/Home.jsx","original":{"line":31,"column":0}},{"name":"Home","generated":{"line":114,"column":15},"source":"pages/Home/Home.jsx","original":{"line":75,"column":15}}],"sources":{"pages/Home/Home.jsx":"import React, { useState, useCallback, useEffect } from \"react\";\nimport Search from \"#src/materialui/Search\";\nimport gql from \"graphql-tag\";\nimport { useQuery } from \"@apollo/react-hooks\";\nimport graphqlClient from \"#src/api/graphql\";\nimport _ from \"lodash\";\nimport { useHistory } from \"react-router-dom\";\nimport MangaDetails from \"./MangaDetails\";\n\nconst GETMANGAS = gql`\n  query getMangas($searchTitle: String!) {\n    mangas(searchTitle: $searchTitle) {\n      id\n      image\n      status\n      title\n    }\n  }\n`;\n\nconst THROTTLE_TIME = 1000;\nconst MIN_QUERY_LENGTH = 3;\n\n//replace all non alphanumerical characters with \"-\", and replace all \"--\"\" with \"-\"\"\nconst sanitiseTitle = (title) =>\n  title\n    .toLowerCase()\n    .replace(/[^a-z0-9]/g, \"-\")\n    .replace(/-{2,}/g, \"-\");\n\nconst Home = () => {\n  const [searchQuery, setSearchQuery] = useState(\"\");\n  const [selectedManga, setSelectedManga] = useState(null);\n  const { loading, error, data } = useQuery(GETMANGAS, {\n    skip: searchQuery.length < MIN_QUERY_LENGTH,\n    variables: { searchTitle: searchQuery },\n  });\n\n  //throttle for quick typers,  if they type too fast, it will wait a certain amount of time\n  //before sending back the autocomplete suggestions\n\n  const handleInputChange = useCallback(\n    _.throttle((searchQuery) => {\n      console.log(searchQuery);\n      setSearchQuery(searchQuery);\n    }, THROTTLE_TIME),\n    [setSearchQuery]\n  );\n\n  const manga = useHistory();\n  const onSelect = (_event, v) => {\n    /*     const test = v;\n    test.title = sanitiseTitle(test.title);\n    manga.push(`${test.id}-${test.title}`); */\n    setSelectedManga(v);\n  };\n\n  //options needs to have a default value\n  let options = [];\n  if (data) {\n    options = data.mangas.slice();\n  }\n  return (\n    <div className=\"main-search-container\">\n      <Search\n        onInputChange={(_event, searchQuery) => handleInputChange(searchQuery)}\n        data={options}\n        onSelect={onSelect}\n      />\n      {selectedManga && <MangaDetails manga={selectedManga} />}\n    </div>\n  );\n};\n\nexport default Home;\n"},"lineCount":null}},"error":null,"hash":"b95b88a44fa66e9d500bc68e5da754ec","cacheData":{"env":{}}}