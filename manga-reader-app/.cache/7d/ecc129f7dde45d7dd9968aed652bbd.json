ce":"pages/MangaChapter.jsx","original":{"line":119,"column":70}},{"generated":{"line":131,"column":147},"source":"pages/MangaChapter.jsx","original":{"line":119,"column":110}},{"generated":{"line":131,"column":148},"source":"pages/MangaChapter.jsx","original":{"line":118,"column":14}},{"generated":{"line":131,"column":150},"source":"pages/MangaChapter.jsx","original":{"line":117,"column":21}},{"generated":{"line":132,"column":0},"source":"pages/MangaChapter.jsx","original":{"line":117,"column":21}},{"generated":{"line":133,"column":0},"source":"pages/MangaChapter.jsx","original":{"line":115,"column":10}},{"generated":{"line":133,"column":20},"source":"pages/MangaChapter.jsx","original":{"line":114,"column":8}},{"generated":{"line":133,"column":21},"source":"pages/MangaChapter.jsx","original":{"line":101,"column":6}},{"generated":{"line":133,"column":22},"source":"pages/MangaChapter.jsx","original":{"line":92,"column":4}},{"generated":{"line":133,"column":23},"source":"pages/MangaChapter.jsx","original":{"line":91,"column":2}},{"generated":{"line":134,"column":0},"source":"pages/MangaChapter.jsx","original":{"line":129,"column":1}},{"generated":{"line":134,"column":1},"source":"pages/MangaChapter.jsx","original":{"line":60,"column":0}},{"name":"MangaChapter","generated":{"line":136,"column":15},"source":"pages/MangaChapter.jsx","original":{"line":131,"column":15}}],"sources":{"pages/MangaChapter.jsx":"import React, { useRef, useEffect } from \"react\";\nimport gql from \"graphql-tag\";\nimport { useQuery } from \"@apollo/react-hooks\";\n\nimport CircularProgress from \"@material-ui/core/CircularProgress\";\n\nimport LazyLoadedImage from \"#src/components/LazyLoadedImage\";\n\nimport Button from \"@material-ui/core/Button\";\n\nimport { useHistory } from \"react-router-dom\";\n\nimport { Link } from \"react-router-dom\";\nimport ScrollToTop from \"#src/components/ScrollToTop\";\n//query the chapter info\n\n/* const query = gql`\n  query($chapterId: ID!) {\n    chapter(id: $chapterId) {\n      images {\n        url\n        width\n        height\n      }\n    }\n  }\n`; */\n\nconst navigationQuery = gql`\n  query($mangaId: ID!, $index: Float!) {\n    previousChapter: chapterIndex(mangaId: $mangaId, index: $index) {\n      id\n    }\n\n    nextChapter: chapterIndex(mangaId: $mangaId, index: $index) {\n      id\n    }\n  }\n`;\n\nconst currentChapterQuery = gql`\n  query($mangaId: ID!, $index: Float!) {\n    chapterIndex(id: $mangaId, index: $index) {\n      images {\n        url\n        width\n        height\n      }\n    }\n  }\n`;\n\n/* const chapterNav = () => {\n  const nextChapt = useHistory();\n  history.push(\n    `/${match.params.mangaId}-${match.params.mangaName}/${parseFloat(match.params.chapterIndex) - 1}`\n  )\n}; */\n\nconst MangaChapter = ({ match }) => {\n  /* const { data, loading, error } = useQuery(query, {\n    variables: { chapterId: match.params.chapterId },\n  }); */\n\n  //const imageRef = useRef();\n\n  const { data, loading, error } = useQuery(currentChapterQuery, {\n    variables: { mangaId: match.params.mangaId, index: parseFloat(match.params.chapterIndex) },\n  });\n  const history = useHistory();\n\n  function ScrollToTopOnMount() {\n    useEffect(() => {\n      window.scrollTo(0, 0);\n    }, []);\n\n    return null;\n  }\n\n  if (loading)\n    return (\n      <div className=\"loading-wrapper\">\n        <CircularProgress />\n      </div>\n    );\n\n  if (error) return `${error}`;\n\n  //const nextLink = `/${match.params.mangaId}-${match.params.mangaName}/${parseFloat(match.params.chapterIndex) + 1}`;\n\n  return (\n    <div className=\"manga-chapter-wrapper\" /* ref={imageRef} */>\n      {/* <pre>{JSON.stringify(match.params, null, 2)}</pre> */}\n      <ScrollToTopOnMount />\n\n      {data.chapterIndex.images.reverse().map((image, index) => (\n        <div key={index}>\n          <LazyLoadedImage src={image.url} /* ref={imageRef} */ />\n        </div>\n      ))}\n      <div className=\"nav-button-wrapper\">\n        <div className=\"prev-button-wrapper\">\n          <Button\n            variant=\"contained\"\n            onClick={() =>\n              history.push(\n                `/${match.params.mangaId}-${match.params.mangaName}/${parseFloat(match.params.chapterIndex) - 1}`\n              )\n            }\n          >\n            Previous Chapter\n          </Button>\n        </div>\n        <div className=\"next-button-wrapper\">\n          <Button\n            variant=\"contained\"\n            onClick={() =>\n              history.push(\n                `/${match.params.mangaId}-${match.params.mangaName}/${parseFloat(match.params.chapterIndex) + 1}`\n              )\n            }\n          >\n            Next Chapter\n          </Button>\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default MangaChapter;\n"},"lineCount":null}},"error":null,"hash":"4d8850538ab6035ad5ac418acf1b44ae","cacheData":{"env":{}}}