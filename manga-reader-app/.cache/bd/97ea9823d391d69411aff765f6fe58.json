ground.paper,\n    padding: 5,\n  },\n})); */\n\nconst MangaDetails = ({ manga }) => {\n  //const classes = useStyles();\n  const { data, loading, error } = useQuery(query, {\n    variables: { mangaId: manga.id },\n  });\n  if (loading)\n    return (\n      <div className=\"loading-wrapper\">\n        <CircularProgress />\n      </div>\n    );\n  const mangaChapters = data.manga.info.chapters;\n\n  const chapterListItem = ({ index, style, data }) => (\n    <ListItem button divider key={data[index].id} style={style}>\n      <ListItemText primary={`#${data[index].number} - ${data[index].title}`} />\n    </ListItem>\n  );\n  return (\n    <div className=\"manga-details\">\n      <img\n        className=\"manga-details-image\"\n        src={manga.image}\n        referrerPolicy=\"no-referrer\"\n        //alt={manga.title}\n      />\n      <div className=\"manga-details-description\">\n        <Card>\n          <CardContent>\n            <Typography variant=\"body2\" component=\"p\">\n              {html_entity_decode(data.manga.info.description)}\n            </Typography>\n          </CardContent>\n        </Card>\n        <br />\n        <FixedSizeList\n          height={450}\n          width={500}\n          itemSize={46}\n          itemCount={Object.keys(mangaChapters).length}\n          itemData={mangaChapters}\n        >\n          {/* {mangaChapters.map((chapter) => (\n            <ListItem button divider key={chapter.id} style={style}>\n              <ListItemText primary={chapter.title} />\n            </ListItem>\n          ))} */}\n          {chapterListItem}\n        </FixedSizeList>\n      </div>\n    </div>\n  );\n};\n\nexport default MangaDetails;\n"},"lineCount":null}},"error":null,"hash":"8b011febebf91ebd649aaac8d6f92e91","cacheData":{"env":{}}}