r key={data[index].id} style={style}>\n      <ListItemText primary={data[index].title} />\n    </ListItem>\n  );\n  return (\n    <div className=\"manga-details\">\n      <img\n        className=\"manga-details-image\"\n        src={manga.image}\n        //alt={manga.title}\n      />\n      <div className=\"manga-details-description\">\n        <Card>\n          <CardContent>\n            <Typography variant=\"body2\" component=\"p\">\n              {data.manga.info.description}\n            </Typography>\n          </CardContent>\n        </Card>\n        <br />\n        <FixedSizeList\n          height={450}\n          width={500}\n          itemSize={46}\n          itemCount={Object.keys(mangaChapters).length}\n          itemData={mangaChapters}\n        >\n          {/* {mangaChapters.map((chapter) => (\n            <ListItem button divider key={chapter.id} style={style}>\n              <ListItemText primary={chapter.title} />\n            </ListItem>\n          ))} */}\n          {chapterListItem}\n        </FixedSizeList>\n      </div>\n    </div>\n  );\n};\n\nexport default MangaDetails;\n"},"lineCount":null}},"error":null,"hash":"598aa3e1ba20c693943ae2ecc11cd39c","cacheData":{"env":{}}}